{
    "appId": "com.geez.music",
    "appName": "GE'EZ music",
    "webDir": "dist",
    "server": {
        "androidScheme": "https"
    },
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 0,
            "backgroundColor": "#0a0a15",
            "showSpinner": false,
            "androidSplashResourceName": "splash",
            "splashFullScreen": true,
            "splashImmersive": true
        },
        "CapacitorHttp": {
            "enabled": true
        }
    },
    "android": {
        "backgroundColor": "#0a0a15",
        "allowMixedContent": true,
        "webContentsDebuggingEnabled": false
    }
}